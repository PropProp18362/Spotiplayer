{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    },
    {
      "src": "vercel-build.js",
      "use": "@vercel/node",
      "config": {
        "distDir": "build/web"
      }
    }
  ],
  "routes": [
    {
      "src": "/login",
      "dest": "server.js"
    },
    {
      "src": "/auth/callback",
      "dest": "server.js"
    },
    {
      "src": "/refresh_token",
      "dest": "server.js"
    },
    {
      "src": "/(.*)",
      "dest": "build/web/$1"
    }
  ],
  "env": {
    "REDIRECT_URI": "https://spotiplayer-tau.vercel.app/auth/callback"
  }
}